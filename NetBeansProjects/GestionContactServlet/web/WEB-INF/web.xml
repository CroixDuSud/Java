<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletAffichageAccueil</servlet-name>
        <servlet-class>ServletAffichageAccueil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAffichageModif</servlet-name>
        <servlet-class>ServletAffichageModif</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAffichageListe</servlet-name>
        <servlet-class>ServletAffichageListe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEnregModif</servlet-name>
        <servlet-class>ServletEnregModif</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAffichageEnregModif</servlet-name>
        <servlet-class>ServletAffichageEnregModif</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAccueil</servlet-name>
        <servlet-class>ServletAccueil</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAffichageAccueil</servlet-name>
        <url-pattern>/ServletAffichageAccueil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAccueil</servlet-name>
        <url-pattern>/ServletAccueil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAffichageModif</servlet-name>
        <url-pattern>/ServletAffichageModif</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAffichageListe</servlet-name>
        <url-pattern>/ServletAffichageListe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEnregModif</servlet-name>
        <url-pattern>/ServletEnregModif</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletAffichageEnregModif</servlet-name>
        <url-pattern>/ServletAffichageEnregModif</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
